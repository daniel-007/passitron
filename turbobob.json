{
	"for_description_of_this_file_see": "https://github.com/function61/turbobob",
	"version_major": 1,
	"project_name": "pi-security-module",
	"builders": [
		{
			"name": "default",
			"uses": "dockerfile://build-default.Dockerfile",
			"mount_destination": "/go/src/github.com/function61/pi-security-module",
			"commands": {
				"dev": ["bash"]
			},
			"dev_ports": ["8091:443"],
			"contextless_build": true
		},
		{
			"name": "frontend",
			"uses": "docker://fn61/buildkit-js:20190128_1731_4cfa8b4e",
			"mount_destination": "/workspace",
			"commands": {
				"build": ["bin/build-frontend.sh"],
				"dev": ["bash"]
			}
		},
		{
			"name": "docs",
			"uses": "docker://fn61/buildkit-mkdocs:20190125_1601_a956d2b7",
			"mount_destination": "/workspace",
			"commands": {
				"build": ["run-mkdocs.sh", "docs/", "rel/docs.tar.gz"],
				"dev": ["bash"]
			}
		},
		{
			"name": "publisher",
			"uses": "docker://fn61/buildkit-publisher:20190125_1635_3730c572",
			"mount_destination": "/workspace",
			"commands": {
				"publish": ["publish.sh", "rel/*"]
			},
			"env": {
				"BINTRAY_PROJECT": "function61/pi-security-module"
			},
			"pass_envs": [
				"BINTRAY_CREDS"
			]
		}
	],
	"os_arches": {
		"linux-amd64": true,
		"linux-arm": true
	}
}
