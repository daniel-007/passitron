{
	"endpoints": [
		{ "path": "/folder/{folderId}", "produces": {"_": "FolderResponse"}, "name": "getFolder" },
		{ "path": "/accounts/{accountId}/secrets/{secretId}/keylist_keys/{key}", "produces": {"_": "SecretKeylistKey"}, "name": "getKeylistKey" },
		{ "path": "/accounts/{accountId}/secrets", "produces": {"_": "list", "of": {"_": "ExposedSecret"}}, "name": "getSecrets" },
		{ "path": "/auditlog", "produces": {"_": "list", "of": {"_": "AuditlogEntry"}}, "name": "auditLogEntries" },
		{ "path": "/accounts/{id}", "produces": {"_": "Account"}, "name": "getAccount" }
	],
	"structs": [
		{
			"name": "FolderResponse",
			"fields": [
				{ "key": "Folder", "type": {"_": "Folder", "nullable": true} },
				{ "key": "SubFolders", "type": {"_": "list", "of": {"_": "Folder"}} },
				{ "key": "ParentFolders", "type": {"_": "list", "of": {"_": "Folder"}} },
				{ "key": "Accounts", "type": {"_": "list", "of": {"_": "Account"}} }
			]
		},
		{
			"name": "Account",
			"fields": [
				{ "key": "Id", "type": {"_": "string"} },
				{ "key": "Created", "type": {"_": "datetime"} },
				{ "key": "FolderId", "type": {"_": "string"} },
				{ "key": "Title", "type": {"_": "string"} },
				{ "key": "Username", "type": {"_": "string"} },
				{ "key": "Description", "type": {"_": "string"} }
			]
		},
		{
			"name": "Folder",
			"fields": [
				{ "key": "Id", "type": {"_": "string"} },
				{ "key": "ParentId", "type": {"_": "string"} },
				{ "key": "Name", "type": {"_": "string"} }
			]
		},
		{
			"name": "AuditlogEntry",
			"fields": [
				{ "key": "Timestamp", "type": {"_": "datetime"} },
				{ "key": "Message", "type": {"_": "string"} }
			]
		},
		{
			"name": "Secret",
			"fields": [
				{ "key": "Id", "type": {"_": "string"} },
				{ "key": "Kind", "type": {"_": "SecretKind"} },
				{ "key": "Title", "type": {"_": "string"} },
				{ "key": "Created", "type": {"_": "datetime"} },
				{ "key": "Password", "type": {"_": "string"} },
				{ "key": "SshPublicKeyAuthorized", "type": {"_": "string"} },
				{ "key": "KeylistKeyPlaceholder", "type": {"_": "string"} }
			]
		},
		{
			"name": "ExposedSecret",
			"fields": [
				{ "key": "Secret", "type": {"_": "Secret"} },
				{ "key": "OtpProof", "type": {"_": "string"} },
				{ "key": "OtpProofTime", "type": {"_": "datetime"} }
			]
		},
		{
			"name": "SecretKeylistKey",
			"fields": [
				{ "key": "Key", "type": {"_": "string"} },
				{ "key": "Value", "type": {"_": "string"} }
			]
		}
	]
}