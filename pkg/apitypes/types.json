				"TrustedFacets": {"_": "list", "of": {"_": "string"}}
			}}
		},
		{
			"name": "U2FSignRequest",
			"type": {"_": "object", "fields": {
				"AppID": {"_": "string"},
				"Challenge": {"_": "string"},
				"RegisteredKeys": {"_": "list", "of": {"_": "U2FRegisteredKey"}}
			}}
		},
		{
